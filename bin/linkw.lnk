# generic linkw.lnk file

# default if no format is defined and 16-bit module is found
system begin 286
    format dos
end

# default if no format is defined and 32-bit module is found
system begin 386
    option  osname='Windows NT character-mode'
    option  map
    libpath %AsmCDir%/lib
    library libc,kernel32,user32
    format  windows nt
    runtime console=4.0
end

# Win32 console application
system begin con_32
    option  quiet
    libpath %ASMCDIR%\lib
    library libc,kernel32,user32
    symt    _cstart
    format  windows pe ^
    runtime console=4.0
end

# Win32 console Unicode application
system begin con_32W
    option  quiet
    libpath %ASMCDIR%\lib
    library libc,kernel32,user32
    symt    _wcstart
    format  windows pe ^
    runtime console=4.0
end

# Win64 console application
system begin con_64
    option  quiet
    libpath %ASMCDIR%\lib\amd64
    library libc,kernel32,user32
    symt    cstart
    format  windows pe ^
    runtime console=4.0
end

# Win64 console Unicode application
system begin con_64W
    option  quiet
    libpath %ASMCDIR%\lib\amd64
    library libc,kernel32,user32
    symt    wcstart
    format  windows pe ^
    runtime console=4.0
end

# Win32 gui application
system begin gui_32
    option  quiet
    libpath %ASMCDIR%\lib
    library libc,kernel32,user32
    symt    WinStart
    format  windows pe ru win ^
    runtime windows=4.0
end

# Win64 gui application
system begin gui_64
    option  quiet
    libpath %ASMCDIR%\lib\amd64
    library libc,kernel32,user32
    symt    WinStart
    format  windows pe ru win ^
    runtime windows=4.0
end

system begin pe_dll
#    option osname='Win32/64 Dll'
    format windows pe dll ^
    runtime windows=4.0
end
