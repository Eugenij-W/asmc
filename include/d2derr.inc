
FACILITY_D2D equ 0x899

MAKE_D2DHR macro sev, code
    exitm<MAKE_HRESULT( sev, FACILITY_D2D, code)>
    endm

MAKE_D2DHR_ERR macro code
    exitm<MAKE_D2DHR( 1, code )>
    endm

